{
  "name": "microsb",
  "version": "1.0.0",
  "description": "Life is like a box of chocolates.",
  "author": "geminiyellow@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc typings coverage dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "test": "karma start './configs/karma/karma.conf.js'",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "build": "rimraf dist && webpack --config './configs/webpack/index.js' --progress --profile",
    "build:watch": "webpack --watch --config './configs/webpack/index.js' --progress --profile",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "prestart": "npm run webdriver-update && npm run build",
    "start": "node ./dist/server/index.js",
    "postinstall": "npm run webdriver-update && typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geminiyellow/microsb.git"
  },
